             aes(x=Distance, y=DR_Ratio,
                 color = Condition)) +
  geom_point() +
  scale_x_log10()+
  geom_line(linewidth = 0.5, alpha = 0.8) +
  scale_x_log10(breaks=c(1.5,2,2.5,3.3,4.3,5.6,7.2,9.4,12,15.5,20,26),
