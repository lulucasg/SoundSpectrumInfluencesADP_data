comleg_condition <- as_ggplot(get_legend(VE_1))+
  theme(plot.margin = margin(0,0,0,0, "cm"))

legends <- ggpubr::ggarrange(comleg_condition)
plot <- ggpubr::ggarrange(BL, DRR, SC, legends,
                          common.legend = TRUE, legend = "none")

